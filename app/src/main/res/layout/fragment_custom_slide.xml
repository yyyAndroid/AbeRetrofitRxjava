<?xml version="1.0" encoding="utf-8"?>
<agency.tango.materialintroscreen.parallax.ParallaxRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:orientation="vertical"
    android:paddingBottom="164dp">
    <ImageView
        android:layout_alignParentRight="true"
        android:layout_marginRight="60dp"
        android:layout_width="120px"
        app:layout_parallaxFactor="0.8"
        android:layout_marginTop="50dp"
        android:layout_height="120px"
        android:src="@mipmap/sun"
        android:id="@+id/imageView3" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="5000px"
        android:src="@mipmap/map"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView4"
        android:layout_marginTop="103dp"
        />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        app:layout_parallaxFactor="0.4"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="60dp"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignBottom="@+id/imageView4"
        android:src="@mipmap/student"
        android:id="@+id/imageView5" />

    <ImageView
        android:layout_width="90px"
        app:layout_parallaxFactor="0.8"
        android:layout_height="90px"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:src="@mipmap/signs"/>

    <ImageView
        android:layout_width="100px"
        android:layout_height="100px"
        app:layout_parallaxFactor="0.5"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="130dp"
        android:src="@mipmap/placeholder"
        android:id="@+id/imageView7" />

<ImageView
        android:layout_width="180px"
        android:layout_height="180px"
        android:layout_marginLeft="20dp"
        android:src="@mipmap/cityscape"
        app:layout_parallaxFactor="0.4"
        android:layout_marginTop="102dp"
        android:id="@+id/imageView6" />




</agency.tango.materialintroscreen.parallax.ParallaxRelativeLayout>
